{
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-invalid-this": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
